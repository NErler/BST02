\newcommand{\emctria@colors@dark}{
  \setbeamercolor{normal text}{%
    fg=white,
    bg=emcdblue
  }
  \usebeamercolor[fg]{normal text}
}
\newcommand{\emctria@colors@light}{
  \setbeamercolor{normal text}{%
    fg=emcdblue,
    bg=white
  }
}
\setbeamercolor{alerted text}{%
  fg=emcred
}
\setbeamercolor{example text}{%
  fg=niheslgreen
}
\setbeamercolor{titlelike}{use=normal text, parent=normal text, fg=emcdblue}
\setbeamercolor{author}{use=normal text, parent=normal text}
\setbeamercolor{date}{use=normal text, parent=normal text, fg=emclblue}
\setbeamercolor{institute}{use=normal text, parent=normal text, fg=emclblue}
\setbeamercolor{structure}{use=normal text, fg=normal text.fg}
\setbeamercolor{palette primary}{%
  use=normal text,
  fg=normal text.fg,
  bg=normal text.bg
}
\setbeamercolor{palette secondary}{%
  fg=white,
  bg=emcdblue
}
\setbeamercolor{palette tertiary}{%
  fg=white,
  bg=emclblue
}
\setbeamercolor{palette quaternary}{%
  fg=emcdblue,
  bg=emclblue
}
\setbeamercolor{frametitle}{%
  use=normal text,
  parent=normal text,
  fg=normal text.fg,
  bg=normal text.bg
}
\setbeamercolor{progress bar}{%
  use=emclblue,
  fg=emclblue,
  bg=emclblue!50!black!30
}
\setbeamercolor{title separator}{
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in head/foot}{%
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in section page}{
  use=progress bar,
  parent=progress bar
}
\newcommand{\emctria@block@transparent}{
  \setbeamercolor{block title}{%
    use=normal text,
    parent=normal text,
    fg=white,
    bg=emcdblue
  }
  \setbeamercolor{block body}{
    use=normal text,
    parent=normal text,
    bg=emclblue
  }
}
\newcommand{\emctria@block@fill}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=normal text.bg!80!fg
  }
  \setbeamercolor{block body}{
    use={block title, normal text},
    bg=block title.bg!50!normal text.bg
  }
}
\setbeamercolor{block title alerted}{%
    use={block title, alerted text},
    bg=block title.bg,
    fg=alerted text.fg
}
\setbeamercolor{block title example}{%
    use={block title, example text},
    bg=block title.bg,
    fg=example text.fg
}
\setbeamercolor{block body alerted}{use=block body, parent=block body}
\setbeamercolor{block body example}{use=block body, parent=block body}
\setbeamercolor{footnote}{fg=normal text.fg!90}
\setbeamercolor{footnote mark}{fg=.}
\setbeamercolor{bibliography entry author}{fg=, bg=}
\setbeamercolor{bibliography entry title}{fg=, bg=}
\setbeamercolor{bibliography entry location}{fg=, bg=}
\setbeamercolor{bibliography entry note}{fg=, bg=}
\emctria@color@setdefaults
\ProcessPgfPackageOptions{/emctria/color}
\mode<all>
