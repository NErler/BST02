\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthemeemctria}[2017/01/23 emctria color theme]
\RequirePackage{pgfopts}
\pgfkeys{
  /emctria/color/block/.cd,
    .is choice,
    transparent/.code=\emctria@block@transparent,
    fill/.code=\emctria@block@fill,
}
\pgfkeys{
  /emctria/color/background/.cd,
    .is choice,
    dark/.code=\emctria@colors@dark,
    light/.code=\emctria@colors@light,
}
\newcommand{\emctria@color@setdefaults}{
  \pgfkeys{/emctria/color/.cd,
    background=light,
    block=transparent,
  }
}
